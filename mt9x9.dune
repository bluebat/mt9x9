#!/usr/bin/dunesh
# 9x9 multiplication table in Dune
# CC0, Wei-Lun Chao <bluebat@member.fsf.org>, 2024.
# dunesh mt9x9.dune

for i in [1, 4, 7] {
    for j in 1 to 10 {
        for k in i to i + 3 {
            print (str k)+"x"+(str j)+"=";
            if k * j < 10 (print " ");
            print (str k * j)+"\t"
        };
        println ""
    };
    echo ""
}
