#!/usr/bin/murex
# 9x9 multiplication table in Murex
# CC0, Wei-Lun Chao <bluebat@member.fsf.org>, 2021.
# ./mt9x9.mx || murex mt9x9

for: (i = 1; i <= 9; i = i + 3) {
    a: [1..9] -> foreach: j {
        a: [$i, ${= i+1}, ${= i+2}] -> foreach: k {
            (${= k}x${= j}=)
            if: {= $k*$j < 10} {( )}
            (${= $k*$j})\t
        }
        ()\n
    }
    out:
}
