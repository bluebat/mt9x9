#!/usr/bin/sagelang
/*
9x9 multiplication table in Sage
CC0, Wei-Lun Chao <bluebat@member.fsf.org>, 2024.
*/
// sagelang mt9x9.sg

for let mut i = 1; i <= 9; i += 3 {
    for let mut j = 1; j <= 9; j += 1 {
        for let mut k = i; k <= i+2; k += 1 {
            print(k, 'x', j, '=');
            if (k*j < 10) {
                print(' ');
            }
            print(k*j, '\t');
        }
        print('\n');
    }
    println();
}
