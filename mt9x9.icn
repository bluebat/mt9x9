#!/usr/bin/icon
# 9x9 multiplication table in Icon
# CC0, Wei-Lun Chao <bluebat@member.fsf.org>, 2020.
# ./mt9x9.icn || icon mt9x9.icn

procedure main()
    local i, j, k
    every i := 1 to 9 by 3 do {
        every j := 1 to 9 do {
            every k := ![i, i+1, i+2] do
                writes(k, "x", j, "=", right(k*j,2), "\t")
            writes("\n")
        }
        write()
    }
end
