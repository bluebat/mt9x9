%!PS
% 9x9 multiplication table in PostScript
% CC0, Wei-Lun Chao <bluebat@member.fsf.org>, 2018
% gs -q -dNODISPLAY -dBATCH mt9x9.ps

1 3 9 {
    /i exch def
    1 1 9 {
        /j exch def
        i 1 i 2 add {
            /k exch def
            k 1 string cvs print
            (x) print
            j 1 string cvs print
            (=) print
            k j mul dup 10 lt {
                ( ) print
            } if
            2 string cvs print
            (	) print
        } for
        () =
    } for
    () =
} for
