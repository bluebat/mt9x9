-- 9x9 multiplication table in Sather
-- CC0, Wei-Lun Chao <bluebat@member.fsf.org>, 2021.
-- sacomp mt9x9.sa -o mt9x9 && ./mt9x9

class MAIN is
    main is
        loop i ::= 1.stepto!(9, 3);
            loop j ::= 1.upto!(9);
                k:ARRAY{INT} := |i, i+1, i+2|;
                loop
                    #OUT+ k.elt! + "x" + j + "=" + #FMT("%2d", k.elt!*j) + "\t"
                end;
                #OUT+ "\n";
            end;
            #OUT+ "\n";
        end
    end
end
